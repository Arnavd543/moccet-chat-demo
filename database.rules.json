{
  "rules": {
    "presence": {
      "$uid": {
        ".read": "auth != null",
        ".write": "$uid === auth.uid"
      }
    },
    "typing": {
      "$channelId": {
        "$uid": {
          ".read": "auth != null",
          ".write": "$uid === auth.uid"
        }
      }
    },
    "status": {
      "$uid": {
        ".read": "auth != null",
        ".write": "$uid === auth.uid"
      }
    }
  }
}